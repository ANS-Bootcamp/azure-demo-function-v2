{
  "bindings": [
    {
      "name": "myBlob",
      "type": "blobTrigger",
      "dataType": "binary",
      "direction": "in",
      "path": "face/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "table",
      "name": "imageTableInfo",
      "tableName": "imageTable",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "dataType": "binary",
      "direction": "out",
      "path": "thumbs/{name}",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "disabled": false
}